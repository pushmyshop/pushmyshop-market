{
    "static.ignore": [
        "^\/assets\/ignore.*$"
    ],
    "routing": {
        "index": "/index.html",
        "routes": {
          "/": {
            "match": "exact"
          },
          "^/.*$": {
            "match": "regex"
          }
        }
    },
    "push": {
      "showNotifications": true,
      "backgroundOnly": false
    } ,
  "dynamic": {
    "group": [{
      "name": "company",
      "urls": {
        ".*/api/compagnies/(0|[1-9][0-9]*)$": {
          "match": "regex"
        }
      },
      "cache": {
        "optimizeFor": "performance",
        "strategy": "lru",
        "maxAgeMs": 3600000,
        "maxEntries": 2
      }
    },
      {
        "name": "products",
        "urls": {
          ".*/api/compagnies/(0|[1-9][0-9]*)/products$": {
            "match": "regex"
          }
        },
        "cache": {
          "optimizeFor": "freshness",
          "strategy": "fifo",
          "maxAgeMs": 3600000,
          "maxEntries": 2,
          "networkTimeoutMs": 1000
        }
      }
    ]
  }

}
