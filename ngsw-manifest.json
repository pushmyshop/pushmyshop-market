{
    "static.ignore": [
        "^\/assets\/ignore.*$"
    ],
    "routing": {
        "index": "/index.html",
        "routes": {
          "/": {
            "match": "exact"
          },
          "^/.*$": {
            "match": "regex"
          }
        }
    },
    "push": {
      "showNotifications": true,
      "backgroundOnly": false
    }

}
